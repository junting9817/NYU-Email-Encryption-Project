  "preferences": {
    "security": {
      "display_decrypted": "inline",
      "hide_armored_header": false,
      "password_cache": true,
      "password_timeout": 30
    },
    "general": {
      "auto_add_primary": true,
      "auto_sign_msg": true,
      "prefer_gnupg": true
    },
    "provider": {
      "gmail_integration": true
    },
    "keyserver": {
      "autocrypt_lookup": false,
      "key_binding": true,
      "mvelo_tofu_lookup": true,
      "oks_lookup": true,
      "wkd_blacklist":
